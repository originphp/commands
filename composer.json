{
    "name": "originphp/commands",
    "description": "Console commands for the OriginPHP framework",
    "type": "originphp-plugin",
    "keywords": [
        "originphp",
        "commands"
    ],
    "homepage": "https://www.originphp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Jamiel Sharief",
            "email": "js@originphp.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Commands\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Commands\\Test\\Fixture\\": "tests/Fixture",
            "Commands\\Test\\": "tests/TestCase"
        }
    },
    "require": {
        "php": "^7.3",
        "originphp/framework": "^3",
        "originphp/inflector": "^1.0",
        "originphp/plugin-installer": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "suggest": {
        "ext-redis": "Redis",
        "ext-imap": "Imap",
        "ext-mailparse": "Mailparse"
    },
    "minimum-stability": "dev"
}
